   <resources>     
	<!--JAR for the application -->
   	<jar href="Suspension_Notebook_non_fat.jar"/>   
	<!--JAR for the db connector -->
   	<jar href="mysql-connector-java-5.1.7-bin.jar"/>   
	<j2se version="1.6+"
	      href="http://java.sun.com/products/autodl/j2se"/>
   </resources>
   <security>
	   <all-permissions/>
