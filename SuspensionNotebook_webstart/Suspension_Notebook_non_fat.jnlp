<?xml version="1.0" encoding="utf-8"?>
<!-- JNLP File for Suspension Notebook -->

<jnlp spec="1.0+"
	codebase="http://www.stuckeyracing.com/SuspensionNotebook/" 
      href="Suspension_Notebook_non_fat.jnlp">
   <information>
      <title>Suspension Notebook</title>
      <vendor>Stuckey Racing</vendor>
      <description>The Suspension Notebook</description>
      <homepage href="http://java.sun.com/docs/books/tutorial/deployment/webstart/running.html"/>
      <description kind="short">This application provides a Configuration Management System
       for suspension settings.</description>
	<icon href="images/icon.jpg"/>
      <offline-allowed/>
